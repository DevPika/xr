{
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-frontmatter","./plugins/pretty-md.mjs"],
  "entryPoints": ["./src/*"],
  "flattenOutputFiles": true,
  "cleanOutputDir": true,
  "hidePageHeader": true,
  "hideBreadcrumbs": true,
  "hidePageTitle": true,
  "frontmatterGlobals": {
    "layout": "docs"
  },
  "out": "./docs",
}